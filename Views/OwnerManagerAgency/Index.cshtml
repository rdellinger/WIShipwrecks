@model IEnumerable<WIShipwrecks.Models.OwnerManagerAgency>

@{
    ViewBag.Title = "Owner/Manager Agencies";
}

<h2>Owner/Manager Agencies</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.OwnerManagerAgency1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
			<a href="@Url.Action("Details", "OwnerManagerAgency", new { id = item.ID })">
				@Html.DisplayFor(modelItem => item.OwnerManagerAgency1)
			</a>
        </td>
        <td>
				@Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
				@Html.DisplayFor(modelItem => item.Phone)
        </td>
        <td width="70px">
			<a href="@Url.Action("Edit", "OwnerManagerAgency", new { id = item.ID })" class="btn btn-info btn-xs">
				<span class="glyphicon glyphicon-pencil"></span>
            </a>
            <a href="@Url.Action("Delete", "OwnerManagerAgency", new { id = item.ID })" class="btn btn-danger btn-xs">
				<span class="glyphicon glyphicon-trash"></span>
            </a>
        </td>
    </tr>
}

</table>
