@model IEnumerable<WIShipwrecks.Models.AttractionType>

@{
    Layout = "~/Views/Shared/_List.cshtml";
    ViewBag.Title = "Attraction Types";
}

<header class="title one">Types of Attractions</header>
<div class="spacer"></div>

<br /><br />

@*If there are any results, then show them*@
@if (Model.Count() > 0)
{

    <table class="table">

        @foreach (var item in Model)
        {
            <tr>
                <td width="60%">
                    <a href="Attraction/ByAttractionType?attractionType=@item.AttractionType1">
                        @Html.DisplayFor(modelItem => item.AttractionType1)
                    </a>

                </td>
            </tr>
        }

    </table>

}
